      "url": "https://nec.com"
    },
    "keywords": ["csv"],
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "links": [
      {
        "name": "Documentation",
        "url": "https://grafana.github.io/grafana-csv-datasource"
      },
      {
        "name": "Website",
        "url": "https://github.com/grafana/grafana-csv-datasource"
      },
      {
        "name": "License",
        "url": "https://github.com/grafana/grafana-csv-datasource/blob/main/LICENSE"
      }
    ],
    "screenshots": [
      { "name": "Explore (Dark)", "path": "img/dark.png" },
      { "name": "Explore (Light)", "path": "img/light.png" }
    ],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "dependencies": {
    "grafanaDependency": ">=8.4.7",
