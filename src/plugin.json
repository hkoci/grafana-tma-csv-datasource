{
  "$schema": "https://github.com/grafana/grafana/raw/main/docs/sources/developers/plugins/plugin.schema.json",
  "type": "datasource",
  "name": "NEC CoE Viavi TMA500 Test Campaign",
  "id": "tma-csv-datasource",
  "metrics": true,
  "backend": true,
  "logs": true,
  "alerting": true,
  "annotations": true,
